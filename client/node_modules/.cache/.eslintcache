  return (\n      <main>\n        <Switch>\n          <Route exact path=\"/\">\n            <NavBar user={user} setUser={setUser} />\n            {/* <Home user={user} /> */}\n            <Emergency\n              user={user.students}\n              handleNewEmergency={handleNewEmergency}\n            />\n          </Route>\n          <Route exact path=\"/emergency\">\n            <NavBar user={user} setUser={setUser} />\n            <Emergency\n              user={user.students}\n              handleNewEmergency={handleNewEmergency}\n            />\n          </Route>\n          <Route path=\"/schedule\">\n            <NavBar user={user} setUser={setUser} />\n            <Schedule />\n          </Route>\n          <Route path=\"/profile\">\n            <NavBar user={user} setUser={setUser} />\n            <Profile />\n          </Route>\n          <Route path=\"/grades\">\n            <NavBar user={user} setUser={setUser} />\n            <Grades />\n          </Route>\n          <Route exact path=\"/events\">\n            <NavBar user={user} setUser={setUser} />\n            <Events />\n          </Route>\n        </Switch>\n      </main>\n    );\n  } else if (user.teacher === true) {\n    return (\n      <>\n        <Switch>\n          <Route exact path=\"/\">\n            <TeacherNavBar user={user} setUser={setUser} />\n            <Teacher />\n          </Route>\n          <Route path=\"/errs\">\n            <TeacherNavBar user={user} setUser={setUser} />\n            <EmergencyRequest\n              emergencyShow={emergencyShow}\n              setEmergencyRequests={setEmergencyRequests}\n            />\n          </Route>\n        </Switch>\n      </>\n    );\n  } else if (user.admin === true) {\n    return (\n      <>\n        <Switch>\n          <Route exact path=\"/\">\n            <AdminNavBar user={user} setUser={setUser} />\n            <Admin />\n          </Route>\n        </Switch>\n      </>\n    );\n  } else {\n    return (\n      <>\n        <Switch>\n          <Route path=\"/signup\">\n            <SignUp user={user} setUser={setUser} />\n          </Route>\n          <Route>\n            <Login handleLogin={handleLogin} />\n          </Route>\n          <Redirect to=\"/login\" />\n          <Route>\n            <LandingPage />\n          </Route>\n        </Switch>\n      </>\n    );\n  }\n}\n\nexport default App;\n","/Users/ibrahimadiallo/Development/Checkin/client/src/SignUp.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/Events.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/Login.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/Grades.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/Profile.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/Schedule.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/LandingPage.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/AdminNavBar.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/Home.js",["120"],"import React from \"react\";\n\nfunction Home({ user }) {\n  return (\n    <div id=\"welcome\">\n      <h1>Welcome, {user.full_name}!</h1>\n      <img\n        // src=\"client/public/nigelthorn.jpeg\"\n        alt=\"school-image\"\n        height=\"100px\"\n        width=\"250px\"\n      />\n    </div>\n  );\n}\n\nexport default Home;\n","/Users/ibrahimadiallo/Development/Checkin/client/src/NavBar.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/Emergency.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/Teacher.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/EmergencyRequest.js",["121","122","123"],"/Users/ibrahimadiallo/Development/Checkin/client/src/TeacherNavBar.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/Admin.js",["124","125","126"],"/Users/ibrahimadiallo/Development/Checkin/client/src/GradeCard.js",[],"/Users/ibrahimadiallo/Development/Checkin/client/src/EventDisplay.js",[],"/Users/ibrahimadiallo/Development/Checkin/clie